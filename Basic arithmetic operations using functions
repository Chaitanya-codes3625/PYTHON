def add(a,b):
    return a+b
def sub(a,b):
    return a-b
def mul(a,b):
    return a*b
def div(a,b):
    return a/b
x=int(input("Enter the first number: "))
y=int(input("Enter the second number:"))
print("The sum is: ",add(x,y))
print("The difference is: ",sub(x,y))
print("The product is: ",mul(x,y))
print("The quotient is: ",div(x,y))
